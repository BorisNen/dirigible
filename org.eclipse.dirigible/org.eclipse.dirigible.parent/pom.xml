								</goals> <id>restore-version-master</id> <phase>validate</phase> </execution> -->
							<!-- </executions> -->
						</plugin>

						<plugin>
							<groupId>org.apache.maven.plugins</groupId>
							<artifactId>maven-pmd-plugin</artifactId>
							<version>2.5</version>
						</plugin>

					</plugins>
				</pluginManagement>
			</build>
		</profile>


