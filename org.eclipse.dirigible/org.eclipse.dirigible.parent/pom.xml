												<artifactId>
													maven-dependency-plugin
												</artifactId>
												<versionRange>[2.1,)</versionRange>
												<goals>
													<goal>
														copy-dependencies
													</goal>
													<goal>
														unpack-dependencies
													</goal>
												</goals>
											</pluginExecutionFilter>
