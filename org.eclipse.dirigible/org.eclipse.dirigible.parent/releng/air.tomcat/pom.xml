		<groupId>org.eclipse.dirigible.releng</groupId>
		<artifactId>org.eclipse.dirigible.releng.parent</artifactId>
		<version>2.7.170225</version>
		<relativePath>../pom.xml</relativePath>
	</parent>

	<name>Releng - Air</name>
	<artifactId>air</artifactId>
	<packaging>war</packaging>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-eclipse-plugin</artifactId>
				<configuration>
					<wtpversion>1.5</wtpversion>
				</configuration>
			</plugin>

			<plugin>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
				
					<execution>
						<id>external</id>
						<phase>compile</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
							<includeArtifactIds>org.eclipse.dirigible.external.p2</includeArtifactIds>
							<excludes>
								**/*icu*.jar,
								**/*felix*.jar,
								**/*archive*.jar,
								**/*mongo*.jar,
								**/*hamcrest*.jar,
								**/*mockito*.jar,
								**/*objenesis*.jar,
								**/*slf*.jar
							</excludes>
						</configuration>
					</execution>
					
					<execution>
						<id>lib</id>
						<phase>compile</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
							<includeArtifactIds>org.eclipse.dirigible.lib.p2</includeArtifactIds>
							<excludes>
								**/*activation*.jar,
								**/*servlet*.jar,
								**/*bridge_*.jar,
								**/*equinox*.jar,
								**/*osgi*.jar,
								**/*hamcrest*.jar,
								**/*mockito*.jar,
								**/*objenesis*.jar,
								**/*jsoup*.jar
								**/*slf*.jar
							</excludes>
						</configuration>
					</execution>
					
					<execution>
						<id>repository</id>
						<phase>compile</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
							<includeArtifactIds>org.eclipse.dirigible.repository.p2</includeArtifactIds>
						</configuration>
					</execution>

					<execution>
						<id>runtime</id>
						<phase>compile</phase>
						<goals>
							<goal>unpack-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
							<includeArtifactIds>org.eclipse.dirigible.runtime.p2</includeArtifactIds>
						</configuration>
					</execution>

				</executions>
			</plugin>

			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>replace-runtime-url</id>
						<phase>prepare-package</phase>
						<configuration>
							<tasks>
								<replace dir="${project.build.directory}/temp">
									<include name="**/web.xml" />
									<replacefilter token="@RUNTIME_URL@" value="${deployment.context}" />
								</replace>
							</tasks>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>

					<execution>
						<id>copy for distribution</id>
						<phase>install</phase>
						<configuration>
							<tasks>

								<!-- Copy deployable war, if relevant -->
								<copy todir="../../../../dist/${deployment.type}/${artifactId}/">
									<fileset dir="${project.build.directory}">
										<include name="*.war" />
									</fileset>
								</copy>
								
								<!-- Copy executable jar, if relevant -->
								<copy todir="../../../../dist/${deployment.type}/${artifactId}/">
									<fileset dir="${project.build.directory}">
										<include name="*.jar" />
										<exclude name="dont-use.jar" />
									</fileset>
								</copy>

							</tasks>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

		</plugins>
	</build>

