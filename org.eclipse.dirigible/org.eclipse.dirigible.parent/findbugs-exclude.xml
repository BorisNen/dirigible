<FindBugsFilter>

	<Match>
    	<Package name="~org\.eclipse\.ui\.dialogs.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.ide.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.ide\.dialogs.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.ide\.undo.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.internal\.ide.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.internal\.ide\.dialogs.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.internal\.ide\.filesystem.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.internal\.ide\.misc.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.internal\.ide\.registry.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  	<Match>
    	<Package name="~org\.eclipse\.ui\.internal\.ide\.undo.*"/>
    	<Bug pattern="SE_BAD_FIELD"/>
  	</Match>
  
     <!-- All bugs in test classes, except for JUnit-specific bugs -->
     <Match>
      <Class name="~.*\.*Test" />
      <Not>
          <Bug code="IJU" />
      </Not>
     </Match>

</FindBugsFilter>